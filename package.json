{
  "name": "node-typescript-mongoose-boilerplate",
  "version": "1.0.0",
  "description": "Create a Node.js app with Typescript for building production-ready RESTful APIs using Express, by running one command",
  "main": "./src/server.ts",
  "scripts": {
    "dev": "export NODE_ENV=development && concurrently \"tsc -w\" \"nodemon ./build/server.js\"",
    "start": " export NODE_ENV=production && pnpm build && node ./build/server.js",
    "build": "tsc -p .",
    "lint": "eslint ./**/*.ts",
    "lint:fix": "eslint ./**/*.ts --fix",
    "prettier": "prettier --check **/*.ts",
    "prettier:fix": "prettier --write **/*.ts",
    "test": "jest -i --colors --verbose --detectOpenHandles --watchAll"
  },
  "keywords": [],
  "author": "Azeem Khan",
  "license": "MIT",
  "repository": "",
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "compression": "^1.7.4",
    "config": "^3.3.9",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "express": "^4.18.2",
    "helmet": "^6.0.1",
    "http-status": "^1.6.2",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.29.4",
    "mongodb": "^5.1.0",
    "mongoose": "^7.0.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.20",
    "npm": "^9.6.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pino-pretty": "^9.4.0",
    "reflect-metadata": "^0.1.13",
    "supertest": "^6.3.3",
    "winston": "^3.8.2",
    "zod": "^3.21.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/config": "^3.3.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.14.6",
    "@types/passport": "^1.0.12",
    "@types/passport-jwt": "^3.0.8",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.35.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-security": "^1.7.1",
    "jest": "^29.5.0",
    "mongodb-memory-server": "^8.12.0",
    "pm2": "^5.2.2",
    "prettier": "^2.8.4",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
